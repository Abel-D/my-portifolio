{
    "installCommand": "npm install --save --force",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "rewrites": [
          {
            "source": "/(.*)",
            "destination": "/index.html"
          }
        ]
  }